{
	"name": "area9-performance-tests",
	"version": "1.0.0",
	"description": "Automated performance testing framework for Area9 learning platform",
	"main": "src/app.js",
	"scripts": {
		"start": "node src/app.js working",
		"test": "node src/app.js working",
		"show-accounts": "node src/show-accounts.js",
		"priority": "node src/app.js priority",
		"priority-watch": "node src/app.js priority --visible --slow",
		"working": "node src/app.js working --slow",
		"working-watch": "node src/app.js working --visible --slow",
		"all": "node src/app.js all",
		"all-watch": "node src/app.js all --visible --slow",
		"scorm": "node src/app.js single scorm --visible --slow",
		"video": "node src/app.js single video --visible --slow",
		"catalog": "node src/app.js single catalog --visible --slow",
		"analytics": "node src/app.js single unique --visible --slow",
		"project": "node src/app.js single project --visible --slow",
		"class": "node src/app.js single class --visible --slow",
		"create": "node src/app.js single create --visible --slow",
		"priority-cold": "node src/app.js priority --no-cache",
		"priority-cold-watch": "node src/app.js priority --no-cache --visible --slow",
		"working-cold": "node src/app.js working --no-cache --slow",
		"working-cold-watch": "node src/app.js working --no-cache --visible --slow",
		"clear-results": "> results.csv",
		"clear-results-warm": "> results/results-warm.csv",
		"clear-results-cold": "> results/results-cold.csv",
		"clear-results-all": "> results.csv && > results/results-all.csv && > results/results-warm.csv && > results/results-cold.csv"
	},
	"keywords": [
		"area9",
		"performance-testing",
		"selenium",
		"webdriver",
		"automation",
		"testing"
	],
	"author": "Area9 Development Team",
	"license": "ISC",
	"type": "module",
	"engines": {
		"node": ">=16.0.0"
	},
	"dependencies": {
		"chromedriver": "^139.0.3",
		"dotenv": "^17.2.2",
		"selenium-webdriver": "^4.35.0"
	}
}
